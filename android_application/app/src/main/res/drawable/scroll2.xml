<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <gradient android:angle="90" android:startColor="#015e50" android:endColor="#030303" />
            <stroke android:color="#0058857e" android:width="10dp"/>
            <corners android:radius="500dip" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <gradient android:angle="90" android:startColor="#015b4d" android:endColor="#000000" />
            <stroke android:color="#0058857e" android:width="10dp"/>
            <corners android:radius="500dip" />
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <gradient android:angle="90" android:startColor="#007c69" android:endColor="#50ffe2" />
            <stroke android:color="#0058857e" android:width="10dp"/>
            <corners android:radius="500dip" />
        </shape>
    </item>
</selector>